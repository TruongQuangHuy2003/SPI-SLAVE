#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x644d658ab200 .scope module, "test_bench" "test_bench" 2 2;
 .timescale -9 -12;
v0x644d658d7de0_0 .var "clk", 0 0;
v0x644d658d7ea0_0 .var "cs", 0 0;
v0x644d658d7f40_0 .net "data_out", 7 0, v0x644d658b78a0_0;  1 drivers
v0x644d658d7fe0_0 .net "miso", 0 0, v0x644d658b7c70_0;  1 drivers
v0x644d658d8080_0 .var "mosi", 0 0;
v0x644d658d8170_0 .var "rst_n", 0 0;
v0x644d658d8240_0 .var "sck", 0 0;
S_0x644d6586e490 .scope module, "dut" "slave" 2 11, 3 1 0, S_0x644d658ab200;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "mosi";
    .port_info 3 /INPUT 1 "sck";
    .port_info 4 /INPUT 1 "cs";
    .port_info 5 /OUTPUT 1 "miso";
    .port_info 6 /OUTPUT 8 "data_out";
v0x644d658b6d50_0 .var "bit_cnt", 2 0;
v0x644d658b7100_0 .net "clk", 0 0, v0x644d658d7de0_0;  1 drivers
v0x644d658b74d0_0 .net "cs", 0 0, v0x644d658d7ea0_0;  1 drivers
v0x644d658b78a0_0 .var "data_out", 7 0;
v0x644d658b7c70_0 .var "miso", 0 0;
v0x644d65895580_0 .net "mosi", 0 0, v0x644d658d8080_0;  1 drivers
v0x644d65895840_0 .net "rst_n", 0 0, v0x644d658d8170_0;  1 drivers
v0x644d658d77b0_0 .net "sck", 0 0, v0x644d658d8240_0;  1 drivers
v0x644d658d7870_0 .var "shift_reg", 7 0;
E_0x644d6586eff0/0 .event negedge, v0x644d65895840_0;
E_0x644d6586eff0/1 .event posedge, v0x644d658b7100_0;
E_0x644d6586eff0 .event/or E_0x644d6586eff0/0, E_0x644d6586eff0/1;
S_0x644d658d7a30 .scope task, "verify" "verify" 2 21, 2 21 0, S_0x644d658ab200;
 .timescale -9 -12;
v0x644d658d7c00_0 .var "byte", 7 0;
v0x644d658d7d00_0 .var/i "i", 31 0;
E_0x644d6586eb40 .event posedge, v0x644d658b7100_0;
TD_test_bench.verify ;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0x644d658d7d00_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x644d658d7d00_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x644d658d7c00_0;
    %load/vec4 v0x644d658d7d00_0;
    %part/s 1;
    %store/vec4 v0x644d658d8080_0, 0, 1;
    %wait E_0x644d6586eb40;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644d658d8240_0, 0, 1;
    %wait E_0x644d6586eb40;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644d658d8240_0, 0, 1;
    %load/vec4 v0x644d658d7d00_0;
    %subi 1, 0, 32;
    %store/vec4 v0x644d658d7d00_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %wait E_0x644d6586eb40;
    %load/vec4 v0x644d658d7f40_0;
    %load/vec4 v0x644d658d7c00_0;
    %cmp/e;
    %jmp/0xz  T_0.2, 4;
    %vpi_call 2 34 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 35 "$display", "PASSED: Expected data_out 8'h%h, Got data_out 8'h%h", v0x644d658d7c00_0, v0x644d658d7f40_0 {0 0 0};
    %vpi_call 2 36 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %jmp T_0.3;
T_0.2 ;
    %vpi_call 2 38 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 39 "$display", "FAILED: Expected data_out 8'h%h, Got data_out 8'h%h", v0x644d658d7c00_0, v0x644d658d7f40_0 {0 0 0};
    %vpi_call 2 40 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
T_0.3 ;
    %end;
    .scope S_0x644d6586e490;
T_1 ;
    %wait E_0x644d6586eff0;
    %load/vec4 v0x644d65895840_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x644d658b7c70_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x644d658b78a0_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x644d658d7870_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x644d658b6d50_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x644d658b74d0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x644d658d77b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %load/vec4 v0x644d658d7870_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x644d65895580_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x644d658d7870_0, 0;
    %load/vec4 v0x644d658b6d50_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x644d658b6d50_0, 0;
    %load/vec4 v0x644d658b6d50_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_1.6, 4;
    %load/vec4 v0x644d658d7870_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x644d65895580_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x644d658b78a0_0, 0;
T_1.6 ;
T_1.4 ;
    %load/vec4 v0x644d658d7870_0;
    %parti/s 1, 7, 4;
    %assign/vec4 v0x644d658b7c70_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x644d658b7c70_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x644d658ab200;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644d658d7de0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x644d658d7de0_0;
    %inv;
    %store/vec4 v0x644d658d7de0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x644d658ab200;
T_3 ;
    %vpi_call 2 51 "$dumpfile", "test_bench.vcd" {0 0 0};
    %vpi_call 2 52 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x644d658ab200 {0 0 0};
    %vpi_call 2 54 "$display", "---------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 55 "$display", "---------------------------------------TESTBENCH FOR SPI SLAVE-------------------------------------------------------" {0 0 0};
    %vpi_call 2 56 "$display", "---------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644d658d8170_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644d658d8080_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644d658d8240_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644d658d7ea0_0, 0, 1;
    %pushi/vec4 10, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x644d6586eb40;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %load/vec4 v0x644d658d7f40_0;
    %cmpi/e 0, 0, 8;
    %jmp/0xz  T_3.2, 4;
    %vpi_call 2 64 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 65 "$display", "PASSED: Expected data_out 8'h%h, Got data_out 8'h%h", 8'b00000000, v0x644d658d7f40_0 {0 0 0};
    %vpi_call 2 66 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %jmp T_3.3;
T_3.2 ;
    %vpi_call 2 68 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 69 "$display", "FAILED: Expected data_out 8'h%h, Got data_out 8'h%h", 8'b00000000, v0x644d658d7f40_0 {0 0 0};
    %vpi_call 2 70 "$display", "----------------------------------------------------------------------------------------------------------------" {0 0 0};
T_3.3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644d658d8170_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644d658d7ea0_0, 0, 1;
    %wait E_0x644d6586eb40;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 51, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 15, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 85, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 119, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 204, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 17, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 153, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 102, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %wait E_0x644d6586eb40;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x644d658d7c00_0, 0, 8;
    %fork TD_test_bench.verify, S_0x644d658d7a30;
    %join;
    %delay 100000, 0;
    %vpi_call 2 109 "$finish" {0 0 0};
    %end;
    .thread T_3;
